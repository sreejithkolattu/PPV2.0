<core:FragmentDefinition xmlns="sap.m"
	xmlns:custom="sap.m.custom" xmlns:core="sap.ui.core">
		<P13nDialog ok="handleColVisibilitySave" cancel="handleClose" showReset="true"  reset="handleRestorePersonalization" initialVisiblePanelType="sort">
		  <panels>
			  <P13nColumnsPanel title="Column personalisation"  visible="true" addColumnsItem="onAddColumnsItem" visibleItemsTreshold="6"
			  type="columns"
			  items="{/ColumnCollection}" columnsItems="{/ColumnCollection}" >
				  <items>
					<P13nItem columnKey="{path}" text="{text}" visible="{visible}"  />
				  </items>
				  <columnsItems>
				   <P13nColumnsItem columnKey="{path}" index ="{index}" text="{text}" visible="{visible}" />
				  </columnsItems>
			 </P13nColumnsPanel>
		  </panels>
	</P13nDialog>
		
		
	
</core:FragmentDefinition>