<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		class="mainDialog">
		<customHeader>
			<Bar>
				<contentMiddle><Title text="Status"></Title></contentMiddle>
				<contentRight><core:Icon src="sap-icon://decline" press=".closeDialog" class="closeDialog"/></contentRight>
			</Bar>
		</customHeader>
		<buttons>
			<Button text="Ok" press="handleClose"></Button>
		</buttons>
		<content>
			<VBox width="100%" alignItems="Stretch" class="dialogFilterBox">
			<customData id="testMe">
			</customData>			
					<ObjectStatus 
						class="sapUiSmallMarginBottom" 
						text="{/text}"
						state="{/state}"
						icon="{/icon}" />
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>
