<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		class="mainDialog">
		<customHeader>
			<Bar>
				<contentMiddle><Title text="Excel Name"></Title></contentMiddle>
				<contentRight><core:Icon src="sap-icon://decline" press=".closeDialog" class="closeDialog"/></contentRight>
			</Bar>
		</customHeader>
		<buttons>
			<Button text="Ok" press="handleExcelSave">
			</Button>
			<Button text="Cancel" press="handleExcelSave">				
			</Button>
		</buttons>
		<content>
			<VBox width="100%" alignItems="Stretch" class="dialogFilterBox">
				<Label text="File Name" design="Bold" />
				<Input id="fname" design="Bold" text="" tooltip="File name" width="40%" />
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>